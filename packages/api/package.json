{
  "name": "@cal-time-visualizer/api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "deploy": "wrangler deploy",
    "typecheck": "tsc --noEmit",
    "db:migrate": "wrangler d1 execute cal-time-visualizer --remote --file=./src/db/schema.sql",
    "db:migrate:local": "wrangler d1 execute cal-time-visualizer --local --file=./src/db/schema.sql"
  },
  "dependencies": {
    "hono": "^4.0.0",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.28.0"
  }
}
